{
  "importMap": "import_map.json",
  "compilerOptions": {
    "strict": false,
    "lib": []
  },
  "fmt": {
    "files": {
      "exclude": [
        "static"
      ]
    }
  },
  "lint": {
    "files": {
      "exclude": [
        "static",
        "test"
      ]
    }
  },
  "tasks": {
    "dev": "deno run --allow-read=. --allow-net --allow-env --allow-hrtime main.tsx",
    "test": "deno test --allow-read=. --allow-net --allow-env --allow-hrtime"
  }
}
